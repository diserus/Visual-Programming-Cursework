   <string notr="true">/* ===== БАЗА: светлая тема ===== */
QMainWindow {
    background-color: #f5f7fb;
    font-family: &quot;Segoe UI&quot;;
    font-size: 10pt;
    color: #1f2937;
}

QWidget {
    color: #1f2937;
}

/* ===== ЛЕВАЯ ПАНЕЛЬ ===== */
QWidget#sidePanel {
    background-color: #0b5ed7; 
	border-left: none;      /* синий */
    border-radius: 12px;

}

QLabel#lblMenu {
    color: white;
    font-size: 20pt;
    font-weight: 600;
}

QLabel#lblUserName,
QLabel#lblRole {
    color: rgba(255, 255, 255, 220);
    font-size: 10pt;
}

/* Кнопки меню */
QPushButton#btnTests,
QPushButton#btnStats,
QPushButton#btnLogout {
    background: transparent;
    color: white;
    border: none;
    text-align: center;
    padding: 10px 12px;
    border-radius: 8px;
    font-weight: 600;
    font-size: 12pt;

}

/* Hover */
QPushButton#btnTests:hover,
QPushButton#btnStats:hover,
QPushButton#btnLogout:hover {
    background-color: rgba(255, 255, 255, 35);
}

/* Pressed */
QPushButton#btnTests:pressed,
QPushButton#btnStats:pressed,
QPushButton#btnLogout:pressed {
    background-color: rgba(0, 0, 0, 25);
}

/* Чтобы можно было программно подсветить активную кнопку:
   ui-&gt;btnTests-&gt;setProperty(&quot;active&quot;, true); style()-&gt;polish(ui-&gt;btnTests);
*/
QPushButton[active=&quot;true&quot;] {
    background-color: rgba(255, 255, 255, 55);
    border-left: 4px solid #ffffff;
    padding-left: 8px; /* чтобы компенсировать border-left */
}

/* ===== ОБЛАСТЬ СТРАНИЦ (справа) ===== */
QStackedWidget {
    background: transparent;
}

/* ===== ПАНЕЛЬ ФИЛЬТРОВ ===== */
QWidget#filtersPanel {
    background-color: #ffffff;
    border: 1px solid #e5e7eb;
    border-radius: 12px;
}

QLabel#lblTests {
    font-size: 14pt;
    font-weight: 700;
    color: #111827;
}
QLabel#lblSearch, 
QLabel#lblSubject, 
QLabel#lblDifficult{
    font-size: 12pt;
    font-weight: 500;
    color: #111827;
}
/* Поля ввода и комбобоксы */
QLineEdit {
    background-color: #ffffff;
    border: 1px solid #d1d5db;
    border-radius: 8px;
    padding: 6px 10px;
    selection-background-color: #0b5ed7;
}

QLineEdit:focus {
    border: 1px solid #0b5ed7;
}

QComboBox {
    background-color: #ffffff;
    border: 1px solid #d1d5db;
    border-radius: 8px;
    padding: 6px 34px 6px 10px;  /* справа место под стрелку */
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 28px;
    border-left: 1px solid #e5e7eb;
    border-top-right-radius: 8px;
    border-bottom-right-radius: 8px;
    background: transparent;
}

/* Рисуем V-стрелку только рамками */
QComboBox::down-arrow {
    width: 7px;
    height: 7px;
    border-left: 2px solid #0b5ed7;
    border-bottom: 2px solid #0b5ed7;
    margin-right: 1px;
}

/* Hover/focus */
QComboBox:hover {
    border: 1px solid #0b5ed7;
}

QComboBox::drop-down:hover {
    background-color: rgba(11, 94, 215, 10);
}


/* ===== ТАБЛИЦА ТЕСТОВ ===== */
QTableWidget {
    background-color: #ffffff;
    border: 1px solid #e5e7eb;
    border-radius: 12px;
    gridline-color: #eef2f7;
    selection-background-color: rgba(11, 94, 215, 35);
    selection-color: #111827;
}

/* Заголовок таблицы */
QHeaderView::section {
    background-color: #f3f6fb;
    color: #111827;
    padding: 8px 10px;
    border: none;
    border-bottom: 1px solid #e5e7eb;
    font-weight: 600;
}

/* Строки */
QTableWidget::item {
    padding: 6px 8px;
    border: none;
}

QTableWidget::item:hover {
    background-color: rgba(11, 94, 215, 18);
}

/* Скроллбар (аккуратный, синий акцент) */
QScrollBar:vertical {
    background: transparent;
    width: 10px;
    margin: 6px 4px 6px 0px;
}

QScrollBar::handle:vertical {
    background: rgba(11, 94, 215, 120);
    border-radius: 5px;
    min-height: 30px;
}

QScrollBar::handle:vertical:hover {
    background: rgba(11, 94, 215, 170);
}

QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical {
    height: 0px;
}

QScrollBar::add-page:vertical,
QScrollBar::sub-page:vertical {
    background: transparent;
}

/* ===== СТАТУС-БАР (если используешь) ===== */
QStatusBar {
    background-color: #ffffff;
    border-top: 1px solid #e5e7eb;
}

